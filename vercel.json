{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/auth/login",
      "dest": "index.js"
    },
    {
      "src": "/auth/register",
      "dest": "index.js"
    },
    {
      "src": "/auth/refresh",
      "dest": "index.js"
    },
    {
      "src": "/auth/user",
      "dest": "index.js"
    },
    {
      "src": "/posts",
      "dest": "index.js"
    },
    {
      "src": "/posts/:post_id",
      "dest": "index.js"
    },
    {
      "src": "/posts//user/:user_id",
      "dest": "index.js"
    },



    {
      "src": "/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth/login/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth/register/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth/refresh/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts/:post_id/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts//user/:user_id/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://next-frontend-one-xi.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}